{
  "name": "boundary-bytes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "db:seed": "prisma db seed",
    "seed:multi": "tsx prisma/seed-multi-league.ts",
    "seed:wpl": "tsx prisma/seed-multi-league.ts WPL",
    "seed:ipl": "tsx prisma/seed-multi-league.ts IPL",
    "migrate:existing": "tsx prisma/migrate-existing-data.ts",
    "postbuild": "prisma migrate deploy",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --check",
      "git add"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.17",
    "@prisma/client": "^6.13.0",
    "@tanstack/react-query": "^5.79.0",
    "@tanstack/react-query-devtools": "^5.79.0",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.1",
    "@vercel/analytics": "^1.5.0",
    "ai": "^4.3.15",
    "axios": "^1.9.0",
    "csv-parse": "^5.6.0",
    "framer-motion": "^12.23.9",
    "next": "^15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-select": "^5.10.1",
    "react-spinners": "^0.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.17.32",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "9.26.0",
    "eslint-config-next": "15.3.2",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "lint-staged": "^14.0.0",
    "prisma": "^6.13.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
